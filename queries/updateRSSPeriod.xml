<query id="updateRSSPeriod" action="update">
    <tables>
        <table name="livexe_rss" />
    </tables>
    <columns>
        <column name="crawler_time" var="crawler_time" filter="number" notnull="notnull" />
    </columns>
    <conditions>
        <condition operation="equal" column="livexe_rss_srl" var="livexe_rss_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
