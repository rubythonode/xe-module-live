<query id="getRssUrl" action="select">
    <tables>
        <table name="livexe_rss" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" filter="number" />
        <condition operation="equal" column="rss_url" var="rss_url" pipe="and" />
        <condition operation="equal" column="livexe_rss_srl" var="livexe_rss_srl" pipe="and" />
    </conditions>
</query>
