<query id="getTagCount" action="select">
    <tables>
        <table name="livexe_tags" />
    </tables>
    <columns>
        <column name="count(*)" alias='count'/>
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" filter="number" />
    </conditions>
</query>
