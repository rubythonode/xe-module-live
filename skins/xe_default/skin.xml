<?xml version="1.0" encoding="UTF-8"?>
<skin version="0.2">
    <title xml:lang="ko">liveXE Default</title>
    <title xml:lang="en">liveXE Default</title>
    <title xml:lang="jp">liveXE Default</title>
    <title xml:lang="zh-TW">liveXE 預設面板</title>
	<title xml:lang="vi">liveXE mặc định</title>
    <description xml:lang="ko">
        liveXE 기본 스킨
    </description>
    <description xml:lang="en">
        liveXE 기본 스킨
    </description>
    <description xml:lang="jp">
        liveXEデフォールトスキン
    </description>
    <description xml:lang="zh-TW">
        liveXE預設面板
    </description>
	<description xml:lang="zh-TW">
        Giao diện mặc định của liveXE
    </description>
    <version>0.1</version>
    <date>2009-04-03</date>

    <author email_address="developers@xpressengine.com" link="http://xpressengine.com/">
        <name xml:lang="ko">NHN</name>
        <name xml:lang="en">NHN</name>
        <name xml:lang="jp">NHN</name>
        <name xml:lang="zh-TW">NHN</name>
		<name xml:lang="vi">NHN</name>
    </author>

    <license>GPL</license>

    <colorset>
        <color name="default">
            <title xml:lang="ko">기본</title>
            <title xml:lang="en">기본</title>
            <title xml:lang="jp">デフォールト</title>
            <title xml:lang="zh-TW">預設</title>
			<title xml:lang="vi">Mặc định</title>
        </color>
    </colorset>

    <extra_vars>
        <var name="title" type="text">
            <title xml:lang="ko">제목</title>
            <title xml:lang="en">제목</title>
            <title xml:lang="jp">タイトル</title>
            <title xml:lang="zh-TW">標題</title>
			<title xml:lang="vi">Tiêu đề</title>
            <description xml:lang="ko">제목을 적어주세요.</description>
            <description xml:lang="en">제목을 적어주세요.</description>
            <description xml:lang="jp">タイトルを入力して下さい。</description>
            <description xml:lang="zh-TW">請輸入標題。</description>
			<description xml:lang="vi">Hãy nhập tiêu đề chính.</description>
        </var>
        <var name="sub_title" type="text">
            <title xml:lang="ko">부제목</title>
            <title xml:lang="en">부제목</title>
            <title xml:lang="jp">サブタイトル</title>
            <title xml:lang="zh-TW">副標題</title>
			<title xml:lang="vi">Tiêu đề phụ</title>
            <description xml:lang="ko">제목 옆에 나타날 부제목을 적어주세요.</description>
            <description xml:lang="en">제목 옆에 나타날 부제목을 적어주세요.</description>
            <description xml:lang="jp">タイトルの隣に表示する、サブタイトルを入力して下さい。</description>
            <description xml:lang="zh-TW">請輸入顯示於標題旁的副標題。</description>
			<description xml:lang="vi">Tiêu đề phụ này sẽ được hiển thị bên cạnh tiêu đề chính.</description>
        </var>
        <var name="comment" type="textarea">
            <title xml:lang="ko">상세 설명</title>
            <title xml:lang="en">상세 설명</title>
            <title xml:lang="jp">詳細説明</title>
            <title xml:lang="zh-TW">詳細說明</title>
			<title xml:lang="vi">Mô tả chi tiết</title>
            <description xml:lang="ko">제목 아래 표시될 설명을 입력하실 수 있습니다.</description>
            <description xml:lang="en">제목 아래 표시될 설명을 입력하실 수 있습니다.</description>
            <description xml:lang="jp">入力した詳細説明はタイトルの下に表示されます。</description>
            <description xml:lang="zh-TW">請輸入顯示於標題下的詳細說明。</description>
			<description xml:lang="vi">Mô tả này sẽ được hiển thị bên dưới tiêu đề.</description>
        </var>
    </extra_vars>
</skin>

